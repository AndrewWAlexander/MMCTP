#tag WindowCode
	#tag Event
		Sub Open()
		  donothing=True
		  BEAM=Window_BEAM_MainInputs.BEAM
		  
		  
		  
		  
		  Refresh_window
		  donothing=False
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub Refresh_window()
		  if BEAM.RBEAM>=0 Then
		    RadioButton_vertical.Value=True
		  else
		    RadioButton_hor.Value=True
		  end
		  
		  EditField_RBEAM.value = str(Abs(BEAM.RBEAM))
		  EditField_RMINBM.value = str(BEAM.RMINBM)
		  EditField_ZSMAX.value = str(BEAM.ZSMAX)
		  EditField_ZSMIN.value = str(BEAM.ZSMIN)
		  EditField_Path.value = str(BEAM.Filnam)
		  EditField_KeV.value = str(BEAM.EIN)
		  
		  
		  if BEAM.MONOEN=1 Then
		    RadioButton_spec.Value=True
		  else
		    RadioButton_mono.Value=True
		  end
		  
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		BEAM As Class_Beam_Inputfile
	#tag EndProperty

	#tag Property, Flags = &h0
		donothing As boolean = true
	#tag EndProperty


#tag EndWindowCode


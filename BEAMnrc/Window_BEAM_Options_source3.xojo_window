#tag Events RadioButton_hor
	#tag Event
		Sub Action()
		  if me.Value Then
		    
		    if BEAM.RBEAM>0 Then
		      BEAM.RBEAM=-1*BEAM.RBEAM
		    elseif BEAM.RBEAM=0 Then
		      BEAM.RBEAM=-1
		    end
		  end
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events RadioButton_vertical
	#tag Event
		Sub Action()
		  if me.Value Then
		    if BEAM.RBEAM<=0 Then
		      BEAM.RBEAM=-1*BEAM.RBEAM
		    end
		  end
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events EditField_RBEAM
	#tag Event
		Sub TextChange()
		  Dim vv as Single
		  
		  vv=val(me.Text)
		  vv=Abs(vv)
		  
		  if RadioButton_hor.Value Then
		    BEAM.RBEAM=-vv
		  else
		    BEAM.RBEAM=vv
		  end
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events EditField_ZSMIN
	#tag Event

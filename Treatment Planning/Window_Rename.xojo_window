#tag WindowCode
	#tag Event
		Sub Close()
		  Window_Treatment.window_update_plan
		  Window_Treatment.beam_update_beam
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub OpenWin()
		  if Dose Then
		    me.Title="Rename Dose "
		    StaticText_old.value = gRTOG.Plan(Plan_Index).Dose(Window_Treatment.dose_index).dose_name
		    EditField_new.value = gRTOG.Plan(Plan_Index).Dose(Window_Treatment.dose_index).dose_name
		    
		  Else
		    me.Title="Rename Plan "+gRTOG.Plan(Plan_Index).Plan_ID
		    StaticText_old.value = (gRTOG.Plan(Plan_Index).Plan_Name)
		    EditField_new.value = (gRTOG.Plan(Plan_Index).Plan_Name)
		  end
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		Dose As Boolean = true
	#tag EndProperty

	#tag Property, Flags = &h0
		doseindex As Integer
	#tag EndProperty

	#tag Property, Flags = &h0
		Plan As Integer
	#tag EndProperty


#tag EndWindowCode

#tag Events PushButton_Ok
	#tag Event
		Sub Action()
		  //-----------------------------------
		  // Rename Plan or Dose files
		  //-----------------------------------
		  DIm f as FolderItem
		  Dim newname,oldname as String
		  //-----------------------------------
		  
		  NewName=EditField_new.Text
		  
		  While InStr(newname," ")>0
		    newname=Replace(newname," ","-")
		  Wend
		  
		  EditField_new.value = NewName
		  
		  
		  if Dose Then
		    PW_Show=true
		    oldname=gRTOG.Plan(Plan_Index).Dose(Window_Treatment.dose_index).dose_name+".Dose"
		    f=gRTOG.Plan(plan_index).Path
		    f=f.Child(oldname)
		    f.Name = NewName+".Dose"
		    gRTOG.Plan(plan_index).Dose(Window_Treatment.dose_index).dose_name = NewName
		    PW_Show=False
		  else 
		    gRTOG.Plan(Plan_Index).Plan_Name=newname
		    gRTOG.Plan(Plan_Index).Write_McGill_Plan_RP
		  end
		  
		  Window_Rename.Close
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events PushButton_Close
	#tag Event
		Sub Action()
		  Window_Rename.Close
		End Sub
	#tag EndEvent

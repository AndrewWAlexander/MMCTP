#tag Events PushButton_save
	#tag Event
		Sub Action()
		  //--------------------------------------
		  //
		  //
		  //--------------------------------------
		  DIm i,x AS Integer
		  Dim zz,zone as Single
		  Dim dd,ddnew as RTOG_Dose_Distribution
		  //--------------------------------------
		  
		  dd=gRTOG.Plan(Plan_Index).Dose(Window_Treatment.dose_index)
		  
		  ddnew= new RTOG_Dose_Distribution
		  ddnew.SOP_Instance_UID=gDICOM.UID_Make
		  ReDim ddnew.Dose_Distribution(UBound(dd.Dose_Distribution))
		  
		  ddnew.Coord_1_1st_point=val(EditField_1stx.Text)
		  ddnew.Coord_2_1st_point=val(EditField_1sty.Text)
		  ddnew.Coord_3_1st_point=val(EditField_1stZ.Text)
		  ddnew.Depth_Grid=dd.Depth_Grid 
		  ddnew.Horizontal_Grid=dd.Horizontal_Grid
		  ddnew.Vertical_Grid=dd.Vertical_Grid
		  ddnew.Size_of_Dimension1=dd.Size_of_Dimension1
		  ddnew.Size_of_Dimension2=dd.Size_of_Dimension2
		  ddnew.Size_of_Dimension3=dd.Size_of_Dimension3
		  ddnew.SeriesDescription=EditField_Dose_Series.Text
		  
		  
		  zz=val(EditField_dz.Text)
		  zone=val(EditField_1stZ.Text)
		  
		  ddnew.Dmax=dd.Dmax 
		  ddnew.Dmin=dd.Dmin
		  ddnew.Dose_Units=Trim(EditField_Dose_units.Text)
		  ddnew.Dose_name=dd.Dose_name
		  ddnew.Dose_Comment=Trim(EditField_Dose_Comments.Text)
		  ddnew.SOP_Instance_UID=Trim(EditField_Dose_SOP.Text)
		  
		  
		  FOR I=0 TO UBound(dd.Dose_Distribution)
		    ddnew.Dose_Distribution(i)=new RTOG_Dose_Plane
		    
		    ReDim ddnew.Dose_Distribution(i).Points(UBound(dd.Dose_Distribution(i).Points))
		    ReDim ddnew.Dose_Distribution(i).errors(UBound(dd.Dose_Distribution(i).Errors))
		    
		    ddnew.Dose_Distribution(i).Z=zone+i*zz
		    
		    for x=0 to UBound(dd.Dose_Distribution(I).Points)
		      ddnew.Dose_Distribution(i).Points(x)=dd.Dose_Distribution(i).Points(x)
		      if UBound(dd.Dose_Distribution(i).Errors)>=i Then
		        ddnew.Dose_Distribution(i).Errors(x)=dd.Dose_Distribution(i).Errors(x)
		      end
		    next
		  next
		  
		  gRTOG.Plan(Plan_Index).Dose.append ddnew
		  
		  PW_Show=true
		  gRTOG.Plan(Plan_Index).Write_McGill_Dose(UBound(gRTOG.Plan(Plan_Index).Dose))
		  Window_Treatment.Window_Update_Plan
		  PW_Show=false
		  Window_Dose_Info.Close
		End Sub
	#tag EndEvent
#tag EndEvents
